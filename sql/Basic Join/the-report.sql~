#MSSQL
SELECT CASE
           WHEN GRADES.GRADE > 7 THEN STUDENTS.NAME
           ELSE null
           END,
           GRADES.GRADE,
           STUDENTS.MARKS
        FROM STUDENTS, GRADES WHERE STUDENTS.MARKS >= GRADES.MIN_MARK and STUDENTS.MARKS <= GRADES.MAX_MARK   
            ORDER BY GRADES.GRADE DESC, STUDENTS.NAME ASC;
